{
  "name": "alpha-loop-scraper",
  "type": "module",
  "scripts": {
    "build": "npm run build:loops-aave && npm run build:loops-compound && npm run build:loops-morpho && npm run build:yields",
    "build:loops-aave": "esbuild ./netlify/functions/loops-aave.ts --bundle --platform=node --outfile=./netlify/functions/loops-aave.js",
    "build:loops-compound": "esbuild ./netlify/functions/loops-compound.ts --bundle --platform=node --outfile=./netlify/functions/loops-compound.js",
    "build:loops-morpho": "esbuild ./netlify/functions/loops-morpho.ts --bundle --platform=node --outfile=./netlify/functions/loops-morpho.js",
    "build:yields": "esbuild ./netlify/functions/yields.ts --bundle --platform=node --outfile=./netlify/functions/yields.js",
    "invoke:loops": "netlify functions:invoke loops",
    "invoke:yields": "netlify functions:invoke yields",
    "generate:abis": "node --experimental-strip-types ./scripts/generate-abis.ts"
  },
  "dependencies": {
    "@bgd-labs/aave-address-book": "^4.24.1",
    "@netlify/cache": "^3.0.5",
    "@netlify/functions": "^4.1.8",
    "graphql-request": "^7.2.0",
    "kysely": "^0.28.2",
    "pg": "^8.16.3",
    "viem": "^2.31.6"
  },
  "devDependencies": {
    "@types/pg": "^8.15.4",
    "esbuild": "^0.25.8",
    "netlify-cli": "^22.2.1"
  }
}
